<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    
    <servlet>
        <servlet-name> regdemo </servlet-name>
        <servlet-class> com.mahboob.backend.Register </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name> regdemo </servlet-name>
        <url-pattern> /reg </url-pattern>
    </servlet-mapping>
     <servlet>
        <servlet-name> logindemo </servlet-name>
        <servlet-class> com.mahboob.backend.Login </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name> logindemo </servlet-name>
        <url-pattern> /login </url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name> addCom </servlet-name>
        <servlet-class> com.mahboob.backend.AddCompany </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name> addCom </servlet-name>
        <url-pattern> /addC </url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name> logout </servlet-name>
        <servlet-class> com.mahboob.backend.Logout </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name> logout </servlet-name>
        <url-pattern> /Logout </url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name> retrieve </servlet-name>
        <servlet-class> com.mahboob.backend.UpdateCompany </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name> retrieve </servlet-name>
        <url-pattern> /ret </url-pattern>
    </servlet-mapping>
</web-app>
